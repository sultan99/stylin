{
  "name": "@stylin/msa-loader",
  "version": "0.4.1",
  "description": "Stylin loader module for webpack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sultan99/stylin",
    "directory": "packages/msa-loader"
  },
  "author": "sultan99",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist && npm run copy-files",
    "compile": "tsc -b",
    "copy-files": "mkdir dist && cp ./src/*.hbs ./dist/",
    "lint": "eslint . --ext .tsx,.ts --fix",
    "prepublishOnly": "npm run build",
    "watch": "tsc -w"
  },
  "devDependencies": {
    "@types/node": "^17.0.24",
    "@types/ramda": "^0.28.11",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-plugin-react": "^7.29.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "handlebars": "^4.7.7",
    "ramda": "^0.28.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
